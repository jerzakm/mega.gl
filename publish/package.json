{
  "name": "mega.gl",
  "version": "0.0.1",
  "description": "Performant math and graphics utilities for TypeScript",
  "license": "MIT",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./math": {
      "types": "./src/math.ts",
      "import": "./src/math.ts"
    }
  },
  "files": [
    "dist",
    "math",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./math": {
        "types": {
          "import": "./dist/math.d.ts",
          "require": "./dist/math.d.cts"
        },
        "import": "./dist/math.js",
        "require": "./dist/math.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@megagl/math": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  }
}
